package data;

import org.newdawn.slick.opengl.Texture;
import static helpers.Artist.*;
public enum TowerType {
	
	RedTower(new Texture[]{quickLoad("tower"),quickLoad("gun")},10),
	BlueTower(new Texture[]{quickLoad("bluetower"),quickLoad("bluegun")},30);
	
	Texture[] textures;
	int damage;
	
	TowerType(Texture[] textures, int damage){
		this.textures = textures;
		this.damage = damage;
	}
}
